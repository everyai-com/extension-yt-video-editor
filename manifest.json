{
  "manifest_version": 3,
  "name": "YouTube Video Extractor",
  "version": "1.0",
  "description": "Extract top 100 video data from a YouTube creator's channel",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}